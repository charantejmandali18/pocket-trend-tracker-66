# Copy this file to .env and fill in your actual OAuth credentials

# Gmail OAuth (from Google Cloud Console)
GMAIL_CLIENT_ID=your-gmail-client-id-here
GMAIL_CLIENT_SECRET=your-gmail-client-secret-here

# Outlook OAuth (from Azure Portal)
OUTLOOK_CLIENT_ID=your-outlook-client-id-here
OUTLOOK_CLIENT_SECRET=your-outlook-client-secret-here

# Security Keys (auto-generated or custom)
ENCRYPTION_KEY=your-secure-encryption-key-here
JWT_SECRET=your-jwt-secret-key-here